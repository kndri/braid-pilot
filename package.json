{
  "name": "braid-pilot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:backend": "dotenv -e .env.local -- convex dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "cypress run",
    "e2e:headed": "cypress open",
    "test:e2e:booking": "cypress run --spec 'cypress/e2e/booking-capacity-flow.cy.ts'",
    "test:e2e:capacity": "cypress run --spec 'cypress/e2e/capacity-braider-management.cy.ts'",
    "test:e2e:all": "cypress run --spec 'cypress/e2e/**/*.cy.ts'",
    "test:cleanup": "node -e \"process.env.CYPRESS_TESTING='true'\" && npm run test:e2e:booking -- --env cleanup=true",
    "test:flow": "node scripts/test-booking-flow.js",
    "seed": "tsx scripts/seed-dev.ts",
    "seed:clean": "tsx scripts/cleanup-dev.ts",
    "test:full": "cypress run --spec 'cypress/e2e/full-salon-experience.cy.ts'",
    "preview:screenshot": "node -e \"process.env.PREVIEW_URL='http://localhost:3000/preview/dashboard';process.env.OUTPUT='public/dashboard-preview.png';process.env.DPR='2';process.env.WIDTH='1600';process.env.HEIGHT='1000'\" && tsx scripts/capture-dashboard-preview.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.30.0",
    "@clerk/themes": "^2.4.6",
    "@heroicons/react": "^2.2.0",
    "@sentry/nextjs": "^10.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.25.4",
    "convex-helpers": "^0.1.101",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "jose": "^6.0.12",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "posthog-js": "^1.260.1",
    "posthog-node": "^5.7.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "^14.5.4",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "puppeteer": "^23.11.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.4",
    "typescript": "^5"
  }
}
